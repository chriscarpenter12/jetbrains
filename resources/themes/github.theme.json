{
  "name": "Github (rainglow)",
  "dark": true,
  "author": "Dayle Rees",
  "editorScheme": "/colors/github.xml",
  "ui": {
    "*": {
      "background": "#404040",
      "foreground": "#ffffff",
      "infoForeground": "#595959",
      "selectionBackground": "#008080",
      "selectionForeground": "#ffffff",
      "selectionInactiveBackground": "#333333",
      "selectionBackgroundInactive": "#ffffff",
      "lightSelectionBackground": "#008080",
      "lightSelectionForeground": "#ffffff",
      "lightSelectionInactiveBackground": "#008080",
      "lightSelectionInactiveForeground": "#ffffff",
      "disabledBackground": "#404040",
      "inactiveBackground": "#404040",
