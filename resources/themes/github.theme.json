    "ComboPopup.border": "#2e2e2e",
    "EditorPane.inactiveBackground": "#333333",
    "EditorTabs": {
      "selectedForeground": "#ffffff",
      "selectedBackground": "#333333",
      "underlineColor": "#333333",
      "inactiveMaskColor": "#404040",
      "borderColor": "#2e2e2e"
    },
    "Link": {
      "activeForeground": "#008080",
      "hoverForeground": "#008080",
      "pressedForeground": "#12404b",
      "visitedForeground": "#12404b"
