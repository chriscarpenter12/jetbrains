      "disabledText": "#595959",
      "inactiveForeground": "#595959",
      "acceleratorForeground": "#ffffff",
      "acceleratorSelectionForeground": "#ffffff",
      "errorForeground": "#4b1717",
      "disabledBorderColor": "#595959",
      "focusColor": "#595959",
      "focusedBorderColor": "#171717",
      "separatorColor": "#404040",
      "lineSeparatorColor": "#404040",
      "modifiedItemForeground": "#008080"
